{
  "loopback-component-explorer": {
	"mountPath": "/explorer"
  },
  "../../../../": {
	"ds": "db",
	"userModel": "Account",
	"rel": "store",
	"groups": [
	  "Store"
	],
	"resources": {
	  "Product": {
		"rel": "owner",
		"polymorphicTypes": [
		  "Store",
		  "User"
		],
		"rowlevel": true
	  }
	}
  },
  "loopback-component-fixtures": {
	"loadFixturesOnStartup": true,
	"environments": [
	  "development",
	  "test"
	],
	"fixturesPath": "test/fixtures/"
  },
  "loopback-component-admin": {
	"mountPath": "/admin",
	"userModel": "Account"
  }
}
