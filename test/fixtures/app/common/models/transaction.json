      "foreignKey": "storeId"
    },
    "invoice": {
      "type": "belongsTo",
      "model": "Invoice",
      "foreignKey": "invoiceId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
