{
  "name": "Transaction",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
		"id": {
			"type": "string",
			"id": true,
			"generated": false
		},
    "storeId": {
      "type": "string",
      "required": true
    },
    "invoiceId": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
